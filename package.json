		"@tiptap/extension-paragraph": "^2.5.4",
		"@tiptap/extension-strike": "^2.5.4",
		"@tiptap/extension-table": "^2.5.2",
		"@tiptap/extension-table-cell": "^2.5.2",
		"@tiptap/extension-table-header": "^2.5.2",
		"@tiptap/extension-table-row": "^2.5.2",
		"@tiptap/extension-text": "^2.5.4",
		"@tiptap/extension-text-align": "^2.5.2",
		"@tiptap/extension-text-style": "^2.5.2",
		"@tiptap/extension-underline": "^2.5.4",
		"@tiptap/pm": "^2.5.1",
		"lit": "^3.2.0"
