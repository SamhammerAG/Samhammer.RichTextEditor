{
	"name": "@samhammer/yarte",
	"version": "1.0.1",
	"description": "Yet another rich text editor based on TipTap and ProseMirror",
	"type": "module",
  "files": ["dist"],
  "main": "./dist/yarte.umd.cjs",
  "module": "./dist/yarte.js",
  "exports": {
    ".": {
      "import": "./dist/yarte.js",
      "require": "./dist/yarte.umd.cjs"
    }
  },
	"scripts": {
		"dev": "vite",
		"watch": "vite build --watch",
		"build": "vite build",
		"preview": "vite preview"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SamhammerAG/yarte"
	},
  "homepage": "https://github.com/SamhammerAG/yarte",
	"keywords": [
		"richtext",
		"editor",
		"tiptap"
	],
	"author": "Samhammer AG",
	"license": "MIT",
	"dependencies": {
		"@tiptap/core": "^2.5.1",
		"@tiptap/extension-bold": "^2.5.4",
		"@tiptap/extension-bubble-menu": "^2.6.2",
		"@tiptap/extension-bullet-list": "^2.5.4",
		"@tiptap/extension-color": "^2.5.2",
		"@tiptap/extension-document": "^2.5.4",
		"@tiptap/extension-highlight": "^2.5.4",
		"@tiptap/extension-history": "^2.5.8",
		"@tiptap/extension-image": "^2.5.2",
		"@tiptap/extension-italic": "^2.5.4",
		"@tiptap/extension-link": "^2.5.4",
		"@tiptap/extension-list-item": "^2.5.8",
